
@model GestionAgroite_V1_CSI.Models.Actividad
@{
    ViewBag.Title = "Visualizar";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

<h2>Visualizar</h2>
<ol class="breadcrumb">
    <li><a href="~/Actividad/Index">Actividad</a></li>
    <li> / </li>
    <li class="active">@Model.IdActividad</li>
</ol>

<div class="card card-body">
    <div class="card-header">Informacion de actividad</div>
    <div class="card-body">
        <dl class="dl-horizontal">
            <dt>Id</dt>
            <dd>@Model.IdActividad</dd>

            <dt>Nombre de Actividad</dt>
            <dd>@Model.Nombre</dd>

        </dl>
    </div>
</div>

