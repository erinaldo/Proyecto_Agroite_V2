
@model List<GestionAgroite_V1_CSI.Models.Frecuencia>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 class="page-header">
    Frecuencia
    <a href="~/Frecuencia/AgregarEditar" class="btn btn-success float-lg-right">Nuevo Frecuencia</a>
</h2>
<br />
<table id="DataTablesLanguage" class="table align-items-center table-flush ">
    <thead>
        <tr>
            <th style="width : 50px;"> ID</th>
            <th style="width : 150px;">Nombre</th>
            <th style="width : 300px">Acciones</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var m in Model)
        {
            <tr>
                <th>@(m.IdFrecuencia)</th>
                <th>@(m.Nombre)</th>
                <th>
                    <a href="~/Frecuencia/Visualizar/@m.IdFrecuencia" class="btn btn-xs btn-warning">Detalles</a>
                    <a href="~/Frecuencia/AgregarEditar/@m.IdFrecuencia" class="btn btn-xs btn-primary">Modificar</a>
                    <a onclick="return confirm('Esta seguro que desea eliminabr el registro?...');"
                       href="~/Frecuencia/Eliminar/@m.IdFrecuencia" class="btn btn-xs btn-danger">Eliminar</a>
                </th>
            </tr>
        }
    </tbody>
</table>

